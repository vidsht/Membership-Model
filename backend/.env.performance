# Backend Performance Optimization Environment Variables
# All flags are OFF by default for safety

# Kill switch - instantly disables all performance optimizations
PERF_DISABLE_ALL=false

# Backend performance flags
PERF_CACHE_HEADERS=false
PERF_COMPRESSION=false
PERF_SECURITY_HEADERS=false

# Development/staging overrides (these can be set to true for testing)
# PERF_CACHE_HEADERS=true
# PERF_COMPRESSION=true
# PERF_SECURITY_HEADERS=true
